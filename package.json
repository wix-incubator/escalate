{
  "private": false,
  "publishConfig": {
    "registry": "http://repo.dev.wix/artifactory/api/npm/npm-local/"
  },
  "name": "gopostal",
  "version": "0.0.59",
  "description": "logger / error reporting facade",
  "main": "dist/src",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:wix/gopostal"
  },
  "scripts": {
    "build:src": "babel ./src/ -d ./dist/src/ --modules umd --source-maps",
    "build:test": "babel ./test/ -d ./dist/test/ --modules umd --source-maps",
    "build:testkit": "babel ./test-kit/ -d ./dist/test-kit/ --modules umd --source-maps",
    "pretest": "npm run build:src && npm run build:test && npm run build:testkit",
    "publish:tc": "ci",
    "test": "mocha --reporter $npm_config_reporter ./dist/test",
    "start": "webpack-dev-server --progress --hot --inline --no-colors"
  },
  "author": "core3",
  "license": "c",
  "devDependencies": {
    "babel": "5.4.7",
    "babel-core": "5.4.7",
    "babel-loader": "5.1.3",
    "bluebird": "3.0.5",
    "chai": "2.3.0",
    "immutable": "3.7.3",
    "mocha": "2.2.5",
    "mocha-loader": "0.7.1",
    "mocha-teamcity-reporter": "0.0.4",
    "node-libs-browser": "0.5.2",
    "sinon": "git://github.com/cjohansen/Sinon.JS.git#b672042043517b9f84e14ed0fb8265126168778a",
    "webpack": "1.9.10",
    "webpack-dev-server": "1.14.1"
  },
  "engines": {
    "node": ">=0.12"
  },
  "dependencies": {
    "lodash": "4.1.0",
    "serialize-javascript": "1.1.2"
  }
}
